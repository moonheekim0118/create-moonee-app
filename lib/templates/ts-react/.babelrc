{
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@emotion/babel-plugin",
      ["module-resolver", {
        "root":["./src"],
        "alias":{
            "utils":"./src/utils",
            "routes":"./src/routes",
            "pages":"./src/pages",
            "components":"./src/components",
            "hooks":"./src/hooks"
        }
    }]
    ]
  }